//https://leetcode.com/problems/design-hashmap/

class MyHashMap {
public:
  
  int m[1000001];
    MyHashMap() {
        memset(m,-1,sizeof(m));
    }
  
    void put(int key, int value) {
        m[key]=value;
    }
    
  
    int get(int key) {
        return m[key];
    }
    
 
    void remove(int key) {
      
            m[key]=-1;
        }
    
};

